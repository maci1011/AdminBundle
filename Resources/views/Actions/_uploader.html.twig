
<form class="maci_uploader_form" action="{{ path("maci_admin_view", {'section': section, 'entity': entity, 'action': 'uploader'}) }}">

	<div class="form-group">

		<label for="file_uploader">File Uploader</label>

		<input id="file_uploader" class="uploader_input" name="file" type="file" multiple="multiple" />

	</div>

	<div class="form-group">

		<a href="#" class="btn btn-default uploader_select">Select Files</a>

	</div>

	<div class="uploader_list"></div>

	<div class="form-group">

		<a href="#" class="btn btn-default uploader_reset">Reset</a>

		<a href="#" class="btn btn-primary uploader_upload">Upload Files</a>

	</div>

</form>
