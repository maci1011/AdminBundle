
<form class="maci-form" action="
	{% if item.id %}
		{{ path("maci_admin_entity_form", {'entity': entity.name, 'id': item.id}) }}
	{% else %}
		{{ path("maci_admin_entity_new", {'entity': entity.name}) }}
	{% endif %}
		" method="post" role="form" {{ form_enctype(form) }}>

    {{ form_rest(form) }}

</form>
